{
  "name": "andersonmadeira/requestor",
  "description": "Consuming RESTful web services easily with php",
  "keywords": [
    "rest",
    "restful",
    "package",
    "web services",
    "comsume",
    "http",
    "client"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Anderson Madeira",
      "email": "andersonmadeiracs@gmail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*"
  },
  "autoload": {
    "psr-4": {
      "AndersonMadeira\\Requestor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndersonMadeira\\Requestor\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
